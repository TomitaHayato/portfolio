<div class="my-3">
  <%= form_with url: routines_path, method: :get do |f| %>
    <div class="flex justify-center">
      <%= f.text_field :user_words, placeholder: '検索ワード 空白で区切ると複数指定できます', value: user_words, class:"input input-bordered w-8/12 text-xs" %>
      <%= f.submit '検索', class: "btn btn-primary text-xs" %>
    </div>
  <% end %>
</div>